function include {
    [[ -s "$1" ]] && source "$1" || echo "$1 not found"
} 


# Path to your oh-my-zsh installation.
export ZSH="/Users/antonkharchevskyi/.oh-my-zsh"
 
ZSH_THEME="robbyrussell"

plugins=(git xcode)

source $ZSH/oh-my-zsh.sh


